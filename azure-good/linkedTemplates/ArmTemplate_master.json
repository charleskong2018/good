{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azure-good"
		},
		"input_blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'input_blob'"
		},
		"input_employee_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'input_employee'"
		},
		"outputDb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'outputDb'"
		},
		"output_employee_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'output_employee'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azure-good_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"input_blob_connectionString": {
						"value": "[parameters('input_blob_connectionString')]"
					},
					"input_employee_connectionString": {
						"value": "[parameters('input_employee_connectionString')]"
					},
					"outputDb_connectionString": {
						"value": "[parameters('outputDb_connectionString')]"
					},
					"output_employee_connectionString": {
						"value": "[parameters('output_employee_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}